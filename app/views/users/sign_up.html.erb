
<div class="wrap">

<h1>Member sign_up</h1>

<%= form_for (@user) do |form|%>

<div class="fields">
  <%= form.label :account, "Account"%>
  <%= form.text_field :account%>
</div>
<div class="fields">
  <%= form.label :password, "Password"%>
  <%= form.password_field :password%>
</div>
<div class="fields">
  <%= form.label :email, "Email"%>
  <%= form.email_field :email%>
</div>

  <%= form.submit "submit"%>

  <%end%>
</div>