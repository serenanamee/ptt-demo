<h1>SIGN_IN</h1>

<%= form_for (@user, url: login_users_path) do |f|%>
<div class="fields">
  <%=f.label :account%>
  <%=f.text_field :account, placeholder:"please put your account and email"%>
</div>
<div class="fields">
  <%=f.label :password%>
  <%=f.password_field :password%>
</div>
  <%=f.submit "sign_in"%>

<%end%>